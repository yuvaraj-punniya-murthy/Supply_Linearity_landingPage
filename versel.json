{
    "build": [{
        "src": "Django_static/wsgi.py",
        "use": "@vercel/python",
        "config": { "maxLambdaSize": "15mb", "runtime": "python 3.10.5"}
    }],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "Django_static/wsgi.py"
        }
    ]
}
